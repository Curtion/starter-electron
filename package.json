{
  "name": "starter-electron",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48",
  "description": "An Electron + Vite + Vue 3 starter template",
  "author": "Curtion",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:dir": "nr build && electron-builder --dir",
    "build:win": "nr build && electron-builder --win",
    "build:linux": "nr build && electron-builder --linux",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/node": "^24.10.1",
    "@unocss/reset": "^66.5.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "builtin-modules": "^5.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "sass-embedded": "^1.97.2",
    "typescript": "~5.9.3",
    "unocss": "^66.5.12",
    "vite": "npm:rolldown-vite@7.2.5",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    },
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller"
    ]
  },
  "build": {
    "appId": "com.curtion.starter-electron",
    "files": [
      "dist-electron/**/*",
      "dist-web/**/*"
    ],
    "win": {
      "icon": "build/logo.png",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "linux": {
      "icon": "build/logo.png",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Utility"
    }
  }
}
